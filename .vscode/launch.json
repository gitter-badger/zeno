  "version": "0.2.0",
  "configurations": [
    {
      "name": "zeno cifar",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "module": "zeno",
      "args": [
        "./examples/cifar_tests.py",
        "--metadata",
        "./examples/models/cifar/cifar_pred.csv",
        "--models",
        "./examples/models/cifar/cifar_net_1.pth",
        "./examples/models/cifar/cifar_net_2.pth",
        "./examples/models/cifar/cifar_net_5.pth",
        "./examples/models/cifar/cifar_net_10.pth",
        "--data-path",
        "~/dev/data/cifar/test/",
        "--batch-size",
        "10"
      ]
    },
    {
      "name": "zeno object",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "module": "zeno",
      "args": [
        "./examples/object_tests.py",
        "--metadata",
        "./examples/validation-images.csv",
        "--models",
        "a",
        "--data-path",
        "~/dev/data/open-images/validation/",
        "--batch-size",
        "10",
        "--id-column",
        "ImageID"
      ]
    }
  ]
}
