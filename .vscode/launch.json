  "version": "0.2.0",
  "configurations": [
    {
      "name": "zeno cifar",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "module": "zeno",
      "args": [
        "./examples/cifar_tests.py",
        "--metadata",
        "./examples/models/cifar/cifar_pred.csv",
        "--models",
        "./examples/models/cifar/cifar_net_1.pth",
        "./examples/models/cifar/cifar_net_2.pth",
        "./examples/models/cifar/cifar_net_5.pth",
        "./examples/models/cifar/cifar_net_10.pth",
        "--data-path",
        "./examples/models/cifar/cif/test/",
        "--batch-size",
        "10"
      ]
    },
    {
      "name": "MLTest Adult",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/zeno.py",
      "args": [
        "--test-files",
        "./adult_tests.py",
        "--metadata",
        "adult/adult_pred.csv",
        "--models",
        "default"
      ]
    }
  ]
}
