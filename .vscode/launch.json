  "version": "0.2.0",
  "configurations": [
    {
      "name": "MLTest",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/mlserve.py",
      "args": [
        "--tests",
        "./amazon_tests.py",
        "--data",
        "sentiment/amazon_pred.csv",
        "--models",
        "siebert/sentiment-roberta-large-english",
        "cardiffnlp/twitter-roberta-base-sentiment",
        "--batch-size",
        "10"
      ]
    },
    {
      "name": "MLTest CIFAR",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/zeno.py",
      "args": [
        "--test-files",
        "./cifar_tests.py",
        "--metadata",
        "cifar/cifar_pred.csv",
        "--models",
        "./cifar/cifar_net.pth",
        "./cifar/cifar_net_1.pth",
        "--data-path",
        "./cifar/cif/test/",
        "--batch-size",
        "10"
      ]
    },
    {
      "name": "MLTest Adult",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/zeno.py",
      "args": [
        "--test-files",
        "./adult_tests.py",
        "--metadata",
        "adult/adult_pred.csv",
        "--models",
        "default"
      ]
    }
  ]
}
