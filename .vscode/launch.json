  "version": "0.2.0",
  "configurations": [
    {
      "name": "zeno cifar",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "module": "zeno",
      "args": [
        "./examples/cifar",
        "--metadata",
        "./examples/models/cifar/cifar_pred.csv",
        "--models",
        "./examples/models/cifar/cifar_net_1.pth",
        "./examples/models/cifar/cifar_net_2.pth",
        "./examples/models/cifar/cifar_net_5.pth",
        "./examples/models/cifar/cifar_net_10.pth",
        "--data-path",
        "~/dev/data/cifar/test/"
      ]
    },
    {
      "name": "zeno coco",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "module": "zeno",
      "args": [
        "./examples/coco",
        "--metadata",
        "./examples/models/coco/coco.parquet",
        "--models",
        "a",
        "--data-path",
        "/Users/acabrera/dev/data/coco-2014/val2014",
        "--batch-size",
        "64",
        "--id-column",
        "file_name"
      ]
    }
  ]
}
